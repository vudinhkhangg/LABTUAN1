import java.util.Scanner;

class bai7 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("nhap 1 so 3 chu so:");
        int n = sc.nextInt();
        int a=n/100; int b=(n%100)/10; int c=n%10; int min=a;
        System.out.println(b);
        if (a>b) min = b;
        if (b>c) min = c;
        System.out.println("so be nhat trong 3 chu so la "+min);
    }
}
